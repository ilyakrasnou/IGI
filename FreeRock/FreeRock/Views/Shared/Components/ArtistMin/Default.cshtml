@model FreeRock.Models.Artist

<h4>Artist</h4>
<div asp-validation-summary="ModelOnly" class="text-danger"></div>
<input type="hidden" asp-for="ArtistID" />
<div class="form-group">
    <label asp-for="Name" class="control-label"></label>
    <input asp-for="Name" class="form-control" disabled />
    <span asp-validation-for="Name" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="Description" class="control-label"></label>
    <input asp-for="Description" class="form-control" disabled/>
    <span asp-validation-for="Description" class="text-danger"></span>
</div>
<div class="form-group">
    <label asp-for="PhotoPath" class="control-label"></label>
    <input asp-for="PhotoPath" class="form-control" disabled/>
    <span asp-validation-for="PhotoPath" class="text-danger"></span>
</div>



